{
  "audit_min_level": "SECURITY",
  "audit_retention": {
    "days": 365,
    "max_files": 1,
    "max_total_mb": 20480,
    "mode": "files"
  },
  "audit_rotate": {
    "check_interval_sec": 10,
    "daily_utc": true,
    "max_active_mb": 256
  },
  "audit_rotation": {
    "max_active_mb": 20,
    "mode": "size_mb",
    "rotate_utc_day": ""
  },
  "user_defaults": {
    "auto_allocate_on_enable": false,
    "default_quota_gb": 10,
    "workspace_layout": "per_fingerprint",
    "workspace_root_dir": "data/workspaces"
  }
}
