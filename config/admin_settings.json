{
  "audit_min_level": "SECURITY",
  "audit_retention": {
    "days": 365,
    "max_files": 1,
    "max_total_mb": 20480,
    "mode": "files"
  },
  "audit_rotate": {
    "check_interval_sec": 10,
    "daily_utc": true,
    "max_active_mb": 256
  },
  "audit_rotation": {
    "max_active_mb": 20,
    "mode": "size_mb",
    "rotate_utc_day": ""
  },
  "snapshots": {
    "enabled": false,

    "backend": "btrfs",

    "volumes": [
      {
        "name": "data",
        "source_subvolume": "/srv/pqnas/data",
        "snap_root": "/srv/pqnas/.snapshots/data"
      }
    ],

    "schedule": {
      "mode": "times_per_day",
      "times_per_day": 6,
      "jitter_seconds": 120
    },

    "retention": {
      "keep_days": 7,
      "keep_min": 12,
      "keep_max": 500
    }
  },
  "ui_theme": "win_classic",
  "transport_max_upload_bytes": 1073741824,
  "user_defaults": {
    "auto_allocate_on_enable": false,
    "default_quota_gb": 10,
    "workspace_layout": "per_fingerprint",
    "workspace_root_dir": "data/workspaces"
  }
}
